    "@remix-run/react": "^2.16.0",
    "@remix-run/serve": "^2.16.0",
    "@types/axios": "^0.14.4",
    "axios": "^1.9.0",
    "isbot": "^4.1.0",
    "langchain": "^0.1.21",
    "openai": "^4.28.0",
    "pg": "^8.15.6",
    "pgvector": "^0.2.0",
    "pino": "^8.0.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^5.10.0",
    "react": "^18.2.0",
